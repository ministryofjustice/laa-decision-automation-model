				<xsd:annotation>
					<xsd:documentation>Record status flag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Rank" type="common:ID40" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to rank classes if multiple classes have been assigned to a customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ActualContentSource" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Source system that users can overwrite data from</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ClassCatagoryUseDetailsElementType">
