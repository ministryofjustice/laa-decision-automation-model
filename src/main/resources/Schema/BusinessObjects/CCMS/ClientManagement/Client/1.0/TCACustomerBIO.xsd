<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:common="http://legalservices.gov.uk/Enterprise/Common/1.0/Common" xmlns:cust="http://legalservices.gov.uk/CCMS/ClientManagement/Client/1.0/TCACustomerBIO" targetNamespace="http://legalservices.gov.uk/CCMS/ClientManagement/Client/1.0/TCACustomerBIO" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:annotation>
		<xsd:documentation xml:lang="en">
			<Version>1.0</Version>
			<Definition>TCA Customer BIO</Definition>
			<Author>Ramesh Madhavan</Author>
			<ModifiedBy>Mohammed Sajid Ali</ModifiedBy>
			<VersionDate>26-04-2010</VersionDate>
			<Description>This schema defines the objects to create or update the Customer at LSC</Description>
		</xsd:documentation>
	</xsd:annotation>
	<xsd:import namespace="http://legalservices.gov.uk/Enterprise/Common/1.0/Common" schemaLocation="../../../../Enterprise/Common/1.0/Common.xsd"/>
	<xsd:complexType name="Party">
		<xsd:sequence>
			<xsd:element name="PartyID" type="cust:PartyIDElementType">
				<xsd:annotation>
					<xsd:documentation>Unique Customer ID or Party Id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartyDetails" type="cust:PartyDetails"/>
			<xsd:element name="Account" type="cust:AccountDetailsElementType" minOccurs="0"/>
			<xsd:element name="RecordHistory" type="common:RecordHistory"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PartyDetails">
		<xsd:sequence>
			<xsd:element name="Reference" type="cust:PartyReferenceDetailsElementType" minOccurs="0"/>
			<xsd:element name="Type" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>Valid values are Person, Organization,Group</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="common:Name" minOccurs="0"/>
			<xsd:element name="OrganizationName" type="common:C35" minOccurs="0"/>
			<xsd:element name="GroupName" type="common:C35" minOccurs="0"/>
			<xsd:element name="KnownAs" type="cust:KnownAsElementType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Customer Alias Names</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneticName" type="common:PhoneticName" minOccurs="0"/>
			<xsd:element name="GeneralInformation" minOccurs="0">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Person" type="cust:PersonTypeDetailsElementType"/>
						<xsd:element name="Organization" type="cust:OrganizationTypeDetailsElementType"/>
						<xsd:element name="Group" type="cust:GroupDetailsElementType"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SpecialNotes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Note" type="cust:NotesElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Locations" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Location" type="cust:LocationDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Languages" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Language" type="cust:LanguageDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Relations" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Relation" type="cust:RelationShipDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PartyPreferences" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Preference" type="cust:ContactPreferenceDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Contacts" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Contact" type="cust:ContactDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Classification" type="cust:CategoryDetailsElementType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PartyUpdate">
		<xsd:sequence>
			<xsd:element name="PartyReference">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="ReferenceID" type="common:IDREF30"/>
						<xsd:element name="ID" type="cust:PartyIDElementType"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Reference" type="cust:PartyReferenceDetailsElementType" minOccurs="0"/>
			<xsd:element name="Name" type="common:Name" minOccurs="0"/>
			<xsd:element name="OrganizationName" type="common:C35" minOccurs="0"/>
			<xsd:element name="GroupName" type="common:C35" minOccurs="0"/>
			<xsd:element name="KnownAs" type="cust:KnownAsElementType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Customer Alias Names
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneticName" type="common:PhoneticName" minOccurs="0"/>
			<xsd:element name="GeneralInformation" minOccurs="0">
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="Person" type="cust:PersonTypeDetailsElementType"/>
						<xsd:element name="Organization" type="cust:OrganizationTypeDetailsElementType"/>
						<xsd:element name="Group" type="cust:GroupDetailsElementType"/>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SpecialNotes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Note" type="cust:NotesElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Sites" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Site" type="cust:SiteDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Locations" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="CustomerLocation" type="cust:LocationDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Languages" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Language" type="cust:LanguageDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Relations" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Relation" type="cust:RelationShipDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PartyPreferences" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ContactPreference" type="cust:ContactPreferenceDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Contacts" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Contact" type="cust:ContactDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Classification" type="cust:CategoryDetailsElementType" minOccurs="0"/>
			<xsd:element name="Account" type="cust:AccountDetailsElementType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonTypeDetailsElementType">
		<xsd:sequence>
			<xsd:element name="PersonalDetails" type="cust:PersonalDetailsElementType" minOccurs="0"/>
			<xsd:element name="OtherDefinedAttributes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="TaxReference" type="common:C50" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Taxpayer registration number, also known as the VAT number</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="JgzzFiscalCode" type="common:C20" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Taxpayer identification number or SSN number or Income taxpayer ID or Fiscal code or NIF</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LastKnownGPS" type="common:C60" minOccurs="0"/>
						<xsd:element name="InternalFlag" type="common:BooleanString" minOccurs="0"/>
						<xsd:element name="FamilyDetails" type="cust:HouseDetailsElementType" minOccurs="0"/>
						<xsd:element name="ActualContentSource" type="common:LOV" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Source System Name</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ConfigurableAttributes" type="common:Attributes20" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>DFF Fields to populate extra information into the e-Business Suite</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonalDetailsElementType">
		<xsd:sequence>
			<xsd:element name="Identifier" type="common:C60" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Personal identifier, such as driver's license or passport number
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdentifierType" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Personal identifier type, such as driver's license or passport</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateOfBirth" type="common:Dt" minOccurs="0"/>
			<xsd:element name="PlaceOfBirth" type="common:C60" minOccurs="0"/>
			<xsd:element name="DateOfDeath" type="common:Dt" minOccurs="0"/>
			<xsd:element name="Gender" type="common:Gender" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Male or Female</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaritalStatus" type="common:LOV" minOccurs="0"/>
			<xsd:element name="MaritalStatusEffectiveDate" type="common:Dt" minOccurs="0"/>
			<xsd:element name="Income" type="common:Amount" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="HouseDetailsElementType">
		<xsd:sequence>
			<xsd:element name="HeadOfHouseholdFlag" type="common:BooleanString" minOccurs="0"/>
			<xsd:element name="HouseHoldIncome" type="common:Amount" minOccurs="0"/>
			<xsd:element name="HouseHoldSize" type="common:Integer" minOccurs="0"/>
			<xsd:element name="RentOwnInd" type="common:BooleanString" minOccurs="0"/>
			<xsd:element name="DeclaredEthnicity" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Society Name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrganizationTypeDetailsElementType">
		<xsd:sequence>
			<xsd:element name="DunsNumber" type="common:IDREF30">
				<xsd:annotation>
					<xsd:documentation>
      A unique identification number assigned to business entities
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EnquiryDuns" type="common:C15" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Headquarters Duns number or Duns Number
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LineOfBusiness" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>
      Type of business activities performed at this site
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrincipalTitle" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Title of the highest ranking person in the company
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrincipalName" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Name of the highest ranking person in the company
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CEOTitle" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Formal title of the chief executive officer
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CEOName" type="common:C35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Name of the chief executive officer
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LegalStatus" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>Legal structure, such as partnership, corporation and so on</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ControlYear" type="common:Integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Year when current ownership gained control</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmployeesTotal" type="common:Integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total number of employees</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HQBranchInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Status of this site. Identifies if the location is the headquarters, a branch, or a single location</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BranchFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Branch location Indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OutOfBusinessInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the organization is out of business or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CongDistCode" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>U.S. Congressional District Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SICCode" type="common:IDREF30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Standard Industry Classification (SIC) code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ImportInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is an importer or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExportInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is an exporter or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LaborSurplusInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company operates in an area with a labor surplus or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MinorityOwnedInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is primarily owned by ethnic or racial minorities</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MinorityOwnedType" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of minority-owned frim</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WomanOwnedInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is primarily owned by women or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DisadvantageInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is considered disadvantaged by the US governement under Title 8A or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SmallBusInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company is considered to be a small business or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RentOwnInd" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this company owns or rents its place of business</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DBRating" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Dun and Bradstreet credit rating</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BestTimeContactBegin" type="common:Dt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Beginning of the best period of time to contact the organization</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BestTimeContactEnd" type="common:Dt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>End of the best period of time to contact the organization</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrganizationNamePhonetic" type="common:C320" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Phonetic representation of organization name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxReference" type="common:C50" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Taxpayer registration number, also known as the VAT number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GSAIndicatorFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this is a US federal agency supported by the General Services Administration (GSA) or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JgzzFiscalCode" type="common:C20" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Taxpayer identification number or SSN number or Income taxpayer ID or Fiscal code or NIF</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnalysisFY" type="common:C5" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fiscal Year</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FiscalYearendMonth" type="common:Integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Month that the fiscal year ends for organization</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrFYPotentialRevenue" type="common:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Organization's potential revenue for this fiscal year</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextFYPotentialRevenue" type="common:Amount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Organization's potential revenue for next fiscal year</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="YearEstablished" type="common:Integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Year that the organization began doing business</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MissionStatement" type="common:C2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Corporate charter of organization</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrganizationType" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>User-defined organization types such as region, division, and department</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BusinessScope" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The physical class of business in which a business participates. For example, local, national, or international
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorporationClass" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Taxation classification for corporation entities</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LocalBusIdentifierType" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup that represents most common business identifier in a country</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LocalBusIdentifier" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Primary identifier assigned to businesses by government agency</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrefFunctionalCurrency" type="common:CurrencyCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Organization's default currency code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegistrationType" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup for type of registration, such as nonprofit</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalEmployeesText" type="common:C60" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total number of personnel employeed</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalEmployeesInd" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup that indicates whether subsidiaries are included in the calculation of total employees or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalEmpEstInd" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup that indicates whether the employee total is estimated or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalEmpMinInd" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup that indicates whether the number is a minimum, maximum, or average number of total employees
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ParentSubInd" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Lookup that indicates if organization is a parent or subsidiary location</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncorpYear" type="common:ID40" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Year that the business was incorporated</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SICCodeType" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Version of Standard Industry Classification (SIC) code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PublicPrivateOwnershipFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
