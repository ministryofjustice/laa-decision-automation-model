	<xsd:complexType name="LanguageDetailsElementType">
		<xsd:sequence>
			<xsd:element name="LanguageReferenceID" type="common:IDREF15" minOccurs="0"/>
			<xsd:element name="LanguageName" type="common:C4">
				<xsd:annotation>
					<xsd:documentation>The standard name for a language. For example, Spanish, Mandarin, or American English</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NativeLanguage" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this language is the person's declared native language or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrimaryLanguageIndicator" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this is the primary language used by a person in day-to-day transactions or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReadsLevel" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>User-defined level number or name to indicate the ability to read a language</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpeaksLevel" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>User-defined level number or name to indicate the ability to speak a language</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WritesLevel" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>User-defined level number or name to indicate the ability to write in a language</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpokenComprehensionLevel" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code to indicate comprehension of a spoken language</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="common:C1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Record status flag</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactPointDetailsElementType">
		<xsd:sequence>
			<xsd:element name="ContactPointID" type="common:ID15" minOccurs="0"/>
			<xsd:element name="ContactPointType" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>Contact point type, such as phone or e-mail</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contact point status, such as active, inactive, delete</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrimaryFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if this is the primary contact point of this contact point type for referenced party, site, or location
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContactPointPurpose" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Contact point type, such as business or personal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrimaryByPurpose" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates this is the primary contact point of the contact point or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="EDI" type="cust:EDIDetailsElementType" minOccurs="0"/>
				<xsd:element name="Email" type="cust:EmailDetailsElementType" minOccurs="0"/>
				<xsd:element name="Phone" type="cust:PhoneDetailsElementType" minOccurs="0"/>
				<xsd:element name="Telex" type="cust:TelexDetailsElementType" minOccurs="0"/>
				<xsd:element name="Web" type="cust:WEBDetailsElementType" minOccurs="0"/>
			</xsd:choice>
			<xsd:element name="ActualContentSource" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Additional Content Source Type column for backward compatibility
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
      			<xsd:element name="ContactPreferences" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Preference" type="cust:ContactPreferenceDetailsElementType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="ConfigurableAttributes" type="common:Attributes20" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      DFF Fields to populate extra information into the e-Business Suite
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EDIDetailsElementType">
		<xsd:sequence>
			<xsd:element name="EDITransactionHandling" type="common:C25" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ANSI standard code for EDI transaction type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIIDNumber" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>EDI trading partner number for the supplier site</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIPaymentMethod" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates how a payment is to be made through EDI</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIPaymentFormat" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates payment format</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIRemittanceMethod" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates how remittance advice are sent to the supplier</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIRemittanceInstruction" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates who will send the remittance advice. Valid values are Banks or Payables systems</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDITPHeaderID" type="common:ID40" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>EDI transaction header unique identifier </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EDIEceTPLocationCode" type="common:C40" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Trading partner location code for EDI Gateway</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmailDetailsElementType">
		<xsd:sequence>
			<xsd:element name="EmailFormat" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the preferred format for e-mail addressed to this address. For example, HTML or ASCII</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmailAddress" type="common:C2000">
				<xsd:annotation>
					<xsd:documentation>E-mail address of the contact point</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PhoneDetailsElementType">
		<xsd:sequence>
			<xsd:element name="PhoneNumber" type="common:C40">
				<xsd:annotation>
					<xsd:documentation>
      A telephone number formatted in the local format. The number should not
      include area code, country code, or extension
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneCallingCalendar" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name or code for a calling calendar used with this telephone number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastContactDtTime" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date and time of last contact placed to this contact point</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneAreaCode" type="common:C10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The area code within a country code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneCountryCode" type="common:C10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>International country code for a telephone number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneExtension" type="common:C20" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional number addressed after initial connection to an internal telephone system</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneLineType" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>Phone line type, such as general, fax, inbound, or outbound</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RawPhoneNumber" type="common:C60" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unformatted telephone number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TelexDetailsElementType">
		<xsd:sequence>
			<xsd:element name="TelexNumber" type="common:C50">
				<xsd:annotation>
					<xsd:documentation>Reference number (usually a telephone number) that TELEX messages are sent to</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="WEBDetailsElementType">
		<xsd:sequence>
			<xsd:element name="WebType" type="common:C60">
				<xsd:annotation>
					<xsd:documentation>The prefix class to a web address. Typically this is either FTP or HTTP</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="URL" type="common:C2000">
				<xsd:annotation>
					<xsd:documentation>Uniform resource locator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RelationTypeDetailsElementType">
		<xsd:sequence>
			<xsd:element name="RelationshipType" type="common:LOV">
				<xsd:annotation>
					<xsd:documentation>The relationship type name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ForwardRelCode" type="common:C30">
				<xsd:annotation>
					<xsd:documentation>The forward relationship code used to describe relationships</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BackwardRelCode" type="common:C30">
				<xsd:annotation>
					<xsd:documentation>The backward relationship code used to describe relationships</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DirectionCode" type="common:C30">
				<xsd:annotation>
					<xsd:documentation>Direction Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HierarchicalFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the relationship type is hierarchy or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreatePartyFlag" type="common:BooleanString" minOccurs="0"/>
			<xsd:element name="AllowRelateToSelfFlag" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if a party can be related to itself or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AllowCircularCelationships" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if circular relationships are allowed using this relationship type or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SubjectType" type="common:C30">
				<xsd:annotation>
					<xsd:documentation>Subject type of the relationship type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ObjectType" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Object type of the relationship type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the relationship type is inactive or active</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MultipleParentAllowed" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether multiple parents are allowed under this relationship type or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InclUnrelatedEntities" type="common:BooleanString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>indicates whether entities not in relationships are included in a hierarchy or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ForwardRole" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This describes the role a subject party plays in a relationship</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BackwardRole" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This describes the role an object party plays in a relationship</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomerRelationShip" type="cust:RelationShipDetailsElementType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CategoryDetailsElementType">
