	<xsd:complexType name="ContactPreferenceDetailsElementType">
		<xsd:sequence>
			<xsd:element name="PreferenceID" type="common:ID15" minOccurs="0"/>
			<xsd:element name="PreferenceCode" type="common:C30">
				<xsd:annotation>
					<xsd:documentation>
      Permission to use or not to use a particular contact method
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContactType" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of contact</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PreferenceStartDate" type="common:Dt">
				<xsd:annotation>
					<xsd:documentation>
      Starting date of the period during which a party should or should not be
      contacted
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PreferenceEndDate" type="common:Dt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Ending date of the period during which a party should or should not be
      contacted
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="common:LOV" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
      Reason that this type of contact should or should not be made
     </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherDefinedAttributes" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PreferenceTopicType" type="common:LOV" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Appropriate subjects to send or not to send to the customer that the preference is created for</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PreferenceTopicTypeCode" type="common:LOV" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Preference topic type code</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PreferenceStartTimeHr" type="common:Tm" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Starting hour of the time range during which a customer should or should not be contacted</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PreferenceEndTimeHr" type="common:Tm" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Ending hour of the time range during which a customer should or should not be contacted.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PreferenceStartTimeHrMi" type="common:Tm" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Starting minute of the time range during which a customer should or should not be contacted</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PreferenceEndTimeHrMi" type="common:Tm" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Ending minute of the time range during which a customer should or should not be contacted</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MaxNoOfInteractions" type="common:Integer" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The number of times you can use this contact point</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MaxNoOfInteractUomCode" type="common:C30" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The period of time during which the maximum number of interactions applies</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RequestedBy" type="common:C30" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates if the permission or restriction was created internally or requested by the customer</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Status" type="common:LOV" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Record status flag</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
