	<xsd:complexType name="ClassCodeAssignDetailsElementType">
		<xsd:sequence>
			<xsd:element name="ClassCategory" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Classification category name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClassCode" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Classification code</xsd:documentation>
