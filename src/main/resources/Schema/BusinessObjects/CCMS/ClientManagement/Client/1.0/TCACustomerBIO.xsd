			</xsd:element>
			<xsd:element name="EndDateActive" type="common:C30" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date when the class code is no longer applied</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
